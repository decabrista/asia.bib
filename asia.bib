@Article{Wang2011c,
  Title                               = {{O}n the origin of {T}ibetans and their genetic basis in adapting high-altitude environments},
  Author                              = {Wang, Binbin and Zhang, Yong-Biao and Zhang, Feng and Lin, Hongbin and Wang, Xumin and Wan, Ning and Ye, Zhenqing and Weng, Haiyu and Zhang, Lili and Li, Xin and Yan, Jiangwei and Wang, Panpan and Wu, Tingting and Cheng, Longfei and Wang, Jing and Wang, Duen-Mei and Ma, Xu and Yu, Jun},
